{
  "env": {
    "es6": true,
    "node": true,
    "mongo": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "func-names": "off",
    "object-shorthand": "off",
    "no-underscore-dangle": "off"
  }
}
